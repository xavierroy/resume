<!doctype html>
<html lang="en-US">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" 
    content="width=device-width, initial-scale=1">

  <link rel="icon" href="./assets/xr-logo.svg" type="image/svg+xml">
  <link rel="apple-touch-icon" href="./assets/xr-logo.svg" type="image/svg+xml">

  <meta property="og:image" content="/assets/xr-resume-og.png">
  <meta property="og:type" content="profile" />
  <meta name="twitter:image" content="/assets/xr-resume-og.png">

  <style>
    {%- include "styles.css" -%}
  </style>
  <title>
    Resume of 
    {{ resume.basics.name }}
  </title>
</head>
<body>
  {% 
    include "components/header.liquid", 
    basics: resume.basics
  %}
  <main>
    {% 
      include "components/summary.liquid", 
      summary: resume.basics.summary 
    %}
    {% 
      include "components/experience.liquid", 
      work: resume.work
    %}
    {% 
      include "components/education.liquid", 
      education: resume.education
    %}
  </main>
</body>

</html>